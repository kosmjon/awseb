{
  "name": "awseb",
  "version": "0.0.1",
  "description":"Sample Express app for CI/CD testing",
  "private": false,
  "repository": {
    "url": "https://github.com/kosmjon/awseb/"
  },
  "scripts": {
    "start": "node ./bin/www",
    "test-old": "./node_modules/.bin/mocha --recursive -R xunit test/ > test-reports.xml",
    "test": "./node_modules/.bin/mocha --recursive --reporter mocha-jenkins-reporter test/"
  },
  "dependencies": {
    "body-parser": "~1.8.1",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "express": "~4.9.0",
    "jade": "~1.6.0",
    "morgan": "~1.3.0",
    "serve-favicon": "~2.1.3",
    "winston": "^0.8.1"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-uglify": "^0.6.0",
    "grunt-simple-mocha": "^0.4.0",
    "mocha": "^2.0.1",
    "should": "^4.1.0",
    "supertest": "^0.14.0",
    "mocha-jenkins-reporter": "0.1.2"
  }
}
